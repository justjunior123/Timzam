<!DOCTYPE html>
<html lang="en">
<head>
 {{>head}}
	<title>ITS ME!</title>
</head>
<body>
	{{>nav}}
	<div class="container main">
		<!-- <h1>{{user.edge_owner_to_timeline_media.edges}}</h1> -->
		<div class="row">
			<div class="col-md-8 col-md-offset-2">
				<div class="row">
					<div class="col-md-4">
						<img class="profile_icon" src="{{user.profile_pic_url}}" alt="">
					</div>
					<div class="col-md-8">
						<h1 class="header">{{user.username}}</h1>

						<div class="row">
							<div class="col-md-4">
								<strong>{{user.edge_follow.count}}</strong><span> Posts</span>
							</div>
							<div class="col-md-4">
								<strong>1.M</strong><span> Followers</span>
							</div>
							<div class="col-md-4">
								<strong>{{user.edge_follow.count}}</strong><span> Following</span>
							</div>
						</div>
						<p style="margin-top: 24px">{{user.biography}}</p>
					</div>
				</div>
				<div class="row">

					{{#user.edge_owner_to_timeline_media.edges}}
					<div class="col-md-4">
						<div class="post">
							<a href="/{{node.shortcode}}">
								<img src="{{node.thumbnail_src}}" alt="">
							</a>
						</div>
					</div>
					{{/user.edge_owner_to_timeline_media.edges}}

				</div>
			</div>
			<!-- <div class="col-md-4">
				<h2>Column 2</h2>
			</div>

			<div class="col-md-4">
				<h2>Column 3</h2>
			</div> -->

		</div>


	</div>

</body>
</html>
